{
	"name": "vscode-navigate-edit-history",
	"displayName": "Vscode Navigate Edit History",
	"description": "TODO",
	"version": "0.0.1",
	"publisher": "codeandstuff",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
        "type": "git",
        "url": "https://github.com/pgsandstrom/vscode-navigate-edit-history.git"
    },
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
            "type": "object",
            "title": "Vscode Navigate Edit History configuration",
            "properties": {
                "navigateEditHistory.maxHistory": {
                    "type": "number",
                    "default": 10,
                    "description": "Maximum number of entries for the edits history."
                },
                "navigateEditHistory.logDebug": {
                    "type": "boolean",
                    "default": false,
                    "description": "Log a bunch of stuff to the console. Use if submitting a bug report."
                }
            }
        },
		"commands": [
			{
				"command": "navigateEditHistory.moveCursorToPreviousEdit",
				"title": "Navigate back in edit history"
			}
		],
		"keybindings": [
			{
				"command": "navigateEditHistory.moveCursorToPreviousEdit",
				"key": "ctrl+q",
				"mac": "cmd+q"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "7.1.1",
		"@types/mocha": "5.2.7",
		"@types/node": "12.11.7",
		"@types/vscode": "1.40.0",
		"glob": "7.1.5",
		"mocha": "6.2.2",
		"typescript": "3.7.2",
		"tslint": "5.20.0",
		"vscode-test": "1.2.2"
	}
}
